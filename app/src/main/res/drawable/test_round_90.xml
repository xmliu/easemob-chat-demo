<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >
    <!-- 填充颜色 -->
    <solid android:color="#ff1570" />
    <!-- 线的宽度，颜色灰色 -->
    <stroke
        android:width="1.5dp"
        android:height="1.5dp"
        android:color="#ff1570" >
    </stroke>
    <size
        android:width="@dimen/width_18_160"
        android:height="@dimen/width_11_160"
        />
    <!-- 矩形的圆角半径 -->
    <corners
        android:topLeftRadius="@dimen/width_5_160"
        android:topRightRadius="@dimen/width_7_160"
        android:bottomLeftRadius="@dimen/width_3_160"
        />
</shape>